{
    "name": "SUPP Chrome Extension",
    "description": "Pair Programming Support Tool",
    "author": "SUPP",
    "version": "1.0.5",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": [
            "*://techcourse.woowahan.com/*/*/*/*"
        ],
        "all_frames": true,
        "js": [
            "./src/js/Content.js"
        ], 
        "run_at": "document_end"
    }],
    "web_accessible_resources": [
        "/src/assets/**",
        "/src/utils/**",
        "/src/js/**"
    ],
    "icons": { 
        "32": "./src/assets/logo/woowa-supp-32.png",
        "64": "./src/assets/logo/woowa-supp-64.png",
        "96": "./src/assets/logo/woowa-supp-96.png",
        "128": "./src/assets/logo/woowa-supp-128.png"
    }
}
