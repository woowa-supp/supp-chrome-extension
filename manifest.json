{
    "name": "SUPP Chrome Extension",
    "description": "Pair Programming Support Tool",
    "author": "SUPP",
    "version": "1.0.0",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": [
            "*://techcourse.woowahan.com/s/*/lt/*",
            "*://techcourse.woowahan.com/s/*/ls/*"
        ],
        "all_frames": true,
        "js": [
            "./src/js/Content.js"
        ], 
        "run_at": "document_end"
    }],
    "web_accessible_resources": [
        "/src/assets/**",
        "/src/utils/**",
        "/src/js/**"
    ],
    "icons": { 
        "30": "./src/assets/logo/supp.png"
    }
}
